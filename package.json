{
  "name": "react-tech-stack-tutorial",
  "version": "1.0.0",
  "description": "react tech stack tutorials",
  "main": "index.js",
  "repository": "git@github.com:/lovesora/react-tech-stack-tutorial.git",
  "author": "liuxin",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:dev": "webpack-dev-server --inline --content-base --history-api-fallback",
    "start:prod": "webpack -p && node server.js",
    "start-srv": "if-env NODE_ENV=production && npm run start-srv:prod || npm run start-srv:dev",
    "start-srv:dev": "webpack-dev-server --inline --content-base --history-api-fallback",
    "start-srv:prod": "npm run build && node server.bundle.js",
    "build:client": "webpack -p",
    "build:server": "webpack --config webpack.server.config.js",
    "build": "npm run build:client && npm run build:server"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.1",
    "file-loader": "^0.11.1",
    "jquery": "^3.2.1",
    "materialize-css": "^0.98.2",
    "node-sass": "^4.5.3",
    "path": "^0.12.7",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "3.0.2",
    "redux": "^3.6.0",
    "redux-devtools": "^3.4.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
